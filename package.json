    "test:coverage": "vitest run --coverage",
    "v": "npm run lint && npm run typecheck && npm run test:run && npm run build"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "happy-dom": "^20.0.11",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "easystarjs": "^0.4.4",
    "lucide-react": "^0.562.0",
    "phaser": "^3.70.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.9"
  }
