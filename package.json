{
  "name": "vibe-game-1",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "deploy": "surge --domain alepha-game.surge.sh dist",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "biome check --write src/",
    "test": "vitest",
